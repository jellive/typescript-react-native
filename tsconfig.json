  "compilerOptions": {
    "target": "es2015",
    "module": "es2015",
    "moduleResolution": "node",
    "jsx": "react",
    "outDir": "compiled",
    "rootDir": "src",
    "types": ["react", "react-native", "jest"]
  },
  "include": ["src/**/*"],
  "exclude": [
	  "android",
	  "ios",
	  "compiled",
	  "node_modules"
   ]
}